{
  "name": "anki-mistral-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "cross-env NODE_OPTIONS='--openssl-legacy-provider --inspect' next start",
    "lint": "next lint",
    "test": "jest",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:fast": "playwright test --grep fast.spec",
    "test:e2e:integration": "playwright test --grep-invert fast.spec",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen http://localhost:3000",
    "test:e2e:chromium": "playwright test --project=chromium"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@mistralai/mistralai": "^1.10.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.0",
    "marked": "^16.4.2",
    "next": "16.0.7",
    "react": "19.2.1",
    "react-csv": "^2.2.2",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.68.0",
    "react-toastify": "^11.0.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "19.2.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.20.0",
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.2"
    }
  }
}
